{
    "name": "francoism90/mediadb",
    "type": "project",
    "description": "Personal media manager.",
    "keywords": [
        "media",
        "stream",
        "mp4"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "beyondcode/laravel-websockets": "^2.0.0-beta.36",
        "elegantweb/sanitizer": "^1.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.0",
        "laravel/framework": "^8.54",
        "laravel/horizon": "^5.7",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.2",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.6",
        "meilisearch/meilisearch-php": "^0.18.3",
        "multicaret/laravel-acquaintances": "^3.5",
        "php-ffmpeg/php-ffmpeg": "^0.18.0",
        "pusher/pusher-php-server": "^5.0",
        "spatie/laravel-db-snapshots": "^2.0",
        "spatie/laravel-json-api-paginate": "^1.10",
        "spatie/laravel-medialibrary": "^9.7",
        "spatie/laravel-permission": "^4.2",
        "spatie/laravel-prefixed-ids": "^1.0",
        "spatie/laravel-query-builder": "^3.5",
        "spatie/laravel-sluggable": "^3.1",
        "spatie/laravel-tags": "^4.0",
        "spatie/laravel-translatable": "^5.0",
        "spatie/temporary-directory": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.10",
        "fakerphp/faker": "^1.14",
        "laravel/sail": "^1.9",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.5",
        "phpstan/phpstan": "^0.12.94",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.11.43"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
